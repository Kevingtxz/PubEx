IFF = University(name='Instituto Federal Fluminense', initials='IFF')
IFF.save()
IFF_address = AddressUniversity(university=IFF.id, neighborhood='Imboassica', city=Macae)
IFF_address.save()

subject_integral = Subject(name='Integral')
subject_integral.save()

discipline_calculus = Discipline(name='Calculus')
discipline_calculus.save()
discipline_calculus.subjects.add(subject_integral)
discipline_calculus.save()

question_1 = Question(title='IFF João Integral', teacher_name='João', university=IFF, subject=subject_integral, text="Faça 1 questões X", right_answear='A')
question_1.save()

question_2 = Question(title='História do Zé', teacher_name='knutsk', university=IFF, text="Faça 2 questões X", right_answear='B')
question_2.save()

question_3 = Question(title='Integral do Leo', teacher_name='Léo', subject=subject_integral, text="Faça 3 questões X", right_answear='C')
question_3.save()

book_calculus = Book(name='Calculus', note="Integral")
book_calculus.save()
book_calculus.questions.add(question_1, question_3)
book_calculus.save()

book_others = Book(name='Others')
book_others.save()
book_others.questions.add(question_2)
book_others.save()

kevin_commentary_question = CommentaryQuestion(question=question_3)
kevin_commentary_question.save()
kevin_commentary_question.text = 'Hi 1'
kevin_commentary_question.save()

knutsk_commentary_question = CommentaryQuestion(question=question_1)
knutsk_commentary_question.save()
knutsk_commentary_question.text = 'Hi 2'
knutsk_commentary_question.save()

commentaries_kevin = Commentaries(standard_user=kevin_standard)
