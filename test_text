Macae = City(name='Macaé', state=RJ)
Macae.save()


kevin_address = Address(number='181', neighborhood='Mirante da Lagoa', complement='Portão branco', city=Macae)
kevin_address.save()

iff_address = Address(number='164', neighborhood='Imboassica', city=Macae)
iff_address.save()

knutsk_address = Address(number='104', neighborhood='Centro', city=Macae)
knutsk_address.save()

leo_address = Address(number='230', neighborhood='Mirante da Lagora', city=Macae)

kevin_standard = StandardUser(user=None, firstname='Kevin', lastname='Trindade Gonçalves', phone='22937841287', email='kevingtxz@gmail.com', sex='M', birth='21101996', address=kevin_address)
kevin_standard.save()

knutsk_standard = StandardUser(user=None, firstname='José', lastname='Knustk', email='knutsk@gmail.com', sex='M', birth='21101990', address=knutsk_address)
knutsk_standard.save()

leo = StandardUser(user=None, firstname='Léo', lastname='Viana', email='leo@gmail.com', sex='M', birth='21101990', address=leo_address)
leo_standard.save()

IFF = University(name='Instituto Federal Fluminense', initials='IFF', address=iff_address)
IFF.save()

subject_integral = Subject(name='Integral')
subject_integral.save()

discipline_calculus = Discipline(name='Calculus')
discipline_calculus.save()
discipline_calculus.subjects.add(subject_integral)
discipline_calculus.save()

question_1 = Question(teacher_name='João', university=IFF, subject=subject_integral, text="Faça 1 questões X", right_answear='A')
question_1.save()

question_2 = Question(teacher_name='knutsk', university=IFF, text="Faça 2 questões X", right_answear='B')
question_2.save()

question_3 = Question(teacher_name='Léo', subject=subject_integral, text="Faça 3 questões X", right_answear='C')
question_3.save()

book_calculus = Book(name='Calculus', note="Calculus", standard_user=kevin_standard)
book_calculus.save()
book_calculus.questions.add(question_1, question_3)
book_calculus.save()

book_others = Book(name='Others', standard_user=knutsk_standard)
book_others.save()
book_others.questions.add(question_2)
book_others.save()

kevin_commentary_question = CommentaryQuestion(question=question_3)
kevin_commentary_question.save()
kevin_commentary_question.text = 'Hi 1'
kevin_commentary_question.save()

knutsk_commentary_question = CommentaryQuestion(question=question_1)
knutsk_commentary_question.save()
knutsk_commentary_question.text = 'Hi 2'
knutsk_commentary_question.save()

commentaries_kevin = Commentaries(standard_user=kevin_standard)
commentaries_kevin.save()
commentaries_kevin.commentaries_questions.add(kevin_commentary_question)
commentaries_kevin.save()

commentaries_knutsk = Commentaries(standard_user=knutsk_standard)
commentaries_knutsk.save()
commentaries_knutsk.commentaries_questions.add(knutsk_commentary_question)
commentaries_knutsk.save()

kevin_like = Likes(standard_user=kevin_standard)
kevin_like.save()
kevin_like.questions.add(question_3, question_2)
kevin_like.save()

kevin_deslike = Deslikes(standard_user=kevin_standard)
kevin_deslike.save()

kevin_deslike.questions.add(question_1)
kevin_deslike.save()

iff_ts = UniversityTeacherStudent(university=IFF)
iff_ts.save()

iff_ts.students.add(kevin)
iff_ts.save()

iff_ts.teachers.add(knutsk)
iff_ts.save()

iff_eq = UniversityQuestion(university=IFF)
iff_eq.save()

iff_eq.questions.add(question_1, question_2)
