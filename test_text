subject_integral = Subject(name='Integral')
subject_integral.save()

discipline_calculus = Discipline(name='Calculus')
discipline_calculus.save()
discipline_calculus.subjects.add(subject_integral)
discipline_calculus.save()




question_1 = Question(teacher_name='João', university=IFF, subject=subject_integral, text="Faça 1 questões X", right_answear='A', education_Level='D')
question_1.save()

question_2 = Question(teacher_name='knutsk', university=IFF, text="Faça 2 questões X", right_answear='B', education_Level='D')
question_2.save()

question_3 = Question(teacher_name='Léo', subject=subject_integral, text="Faça 3 questões X", right_answear='C', education_Level='D')
question_3.save()




book_calculus = Book(name='Calculus', note="Integral")
book_calculus.save()
book_calculus.questions.add(question_1, question_3)
book_calculus.save()

book_others = Book(name='Others')
book_others.save()
book_others.questions.add(question_2)
book_others.save()




kevin_commentary_question = CommentaryQuestion(question=question_3, text = 'Hi 1')
